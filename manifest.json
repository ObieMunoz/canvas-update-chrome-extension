{
    "manifest_version": 2,
    "name": "Canvas Upgrade",
    "author": "Obie Munoz Jr",
    "description": "This extension is meant for use in conjunction with the Flatiron School's Canvas courseware. The extension provides enhanced functionality and visual aids.",
    "version": "0.0.0.7",
    "icons": {
        "16": "./images/favicon-16x16.png",
        "32": "./images/favicon-32x32.png",
        "192": "./images/android-chrome-192x192.png",
        "512": "./images/android-chrome-512x512.png"
    },
    "page_action": {
        "default_icon": {
            "16": "./images/favicon-16x16.png",
            "32": "./images/favicon-32x32.png",
            "192": "./images/android-chrome-192x192.png",
            "512": "./images/android-chrome-512x512.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://learning.flatironschool.com/*",
                "http://learning.flatironschool.com/*"
            ],
            "js": [
                "index.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}